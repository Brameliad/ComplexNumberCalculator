public class ComplexMathTester{
public static void main(String[] args)
{
System.out.print("\f");
ComplexNumber x = new ComplexNumber(2, 3);
System.out.println(x);
ComplexNumber y = new ComplexNumber(3, -4);
System.out.println(y);
System.out.println(x.equals(y));
System.out.println(x.getConjugate());
System.out.println(ComplexMath.add(x,y));
System.out.println(ComplexMath.subtract(x,y));
System.out.println(ComplexMath.multiply(x,y));
System.out.println(ComplexMath.divide(x,y));


}


}
